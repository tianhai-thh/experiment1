<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.itheima.mapper.SchoolMapper">


    <select id="selectcsid" resultType="java.lang.Integer">

        select id from s_school where schoolname = #{schoolname};

    </select>
    <select id="selectAll" resultType="com.itheima.pojo.School">
        select * from s_school;
    </select>


</mapper>